{
   "__id":"insightly",
   "name":"Insightly",
   "description":"Insightly CRM API connector.",
   "status":"alpha",
   "type":"application",
   "supports":[
      "object"
   ],
   "logo":"",
   "connectorType":"http",
   "authType":"oauth2",
   "organization":"insightly",
   "categories":[
      "crm"
   ],
   "config":{
      "list":[
         "baseURL",
         "key"
      ],
      "baseURL":{
         "name":"Base URL",
         "dataType":"string",
         "required":true,
         "icon":"http",
         "defaultValue":"https://api.insightly.com/v2.2",
         "writable":false
      },
      "authType":{
         "name":"Authentication Type",
         "dataType":"string",
         "required":true,
         "hidden":true,
         "icon":"security",
         "defaultValue":"oauth2"
      },
      "oauth2":{
         "list":[
            "accessToken"
         ],
         "accessToken":{
            "name":"Access Token",
            "dataType":"password",
            "icon":"security",
            "required":true
         },
         "options":{
            "list":[
               "headers"
            ],
            "headers":{
               "Authorization":"Bearer ${accessToken}",
               "Content-Type":"application/json"
            },
            "connection":{
               "input":{
                  "list":[
                     "query",
                     "options"
                  ],
                  "query":{
                     "name":"query",
                     "dataType":"query",
                     "required":false
                  },
                  "options":{
                     "name":"Options",
                     "dataType":"object"
                  },
                  "httpOptions":{
                     "preProcessor":{
                        "uriPath":"/insightly"
                     },
                     "postProcessor":{
                        "successStatus":[
                           "200"
                        ]
                     }
                  }
               }
            }
         }
      }
   },
   "functions":{
      "list":[
         "getobjects",
         "getattributes"
      ],
      "getobjects":{
         "__id":"getobjects",
         "name":"Get Objects",
         "functionName":"objects",
         "crudType":"R"
      },
      "getattributes":{
         "__id":"getattributes",
         "name":"Get Attributes",
         "crudType":"R",
         "functionName":"attributes",
         "input":{
            "list":[
               "object"
            ],
            "object":{
               "name":"object",
               "dataType":"string",
               "required":true
            }
         }
      }
   },
   "objects":{
      "list":[
         "contacts",
         "leads",
         "organizations",
         "tasks",
         "teams"
      ],
      "contacts":"${include|contacts.json}",
      "leads":"${include|leads.json}",
      "organizations":"${include|organizations.json}",
      "tasks":"${include|tasks.json}",
      "teams":"${include|teams.json}"
   }
}