{
   "list":[
      "getattributes",
      "get",
      "getById",
      "create"
   ],
   "getattributes":{
      "__id":"getattributes",
      "name":"Get Attributes",
      "functionName":"execute",
      "hidden":true
   },
   "get":{
      "__id":"get",
      "name":"Get",
      "functionName":"execute",
      "crudType":"R",
      "dataFlow":"pull",
      "options":{
         "filter":false,
         "sort":false,
         "maxPageSize":100
      },
      "httpOptions":{
         "method":"get",
         "preProcessor":{
            "uriPath":"/orders"
         }
      },
      "input":{
         "list":[
            "query",
            "options"
         ],
         "query":{
            "name":"query",
            "dataType":"query",
            "required":false
         },
         "options":{
            "name":"options",
            "dataType":"object"
         }
      }
   },
   "getById":{
      "__id":"getById",
      "name":"Get By ID",
      "functionName":"getById",
      "crudType":"R",
      "dataFlow":"pull",
      "httpOptions":{
         "method":"get",
         "preProcessor":{
            "uriPath":"/orders/${id}"
         }
      },
      "input":{
         "list":[
            "id",
            "options"
         ],
         "id":{
            "name":"id",
            "dataType":"string",
            "required":true
         },
         "options":{
            "name":"options",
            "dataType":"object",
            "method":"get"
         }
      }
   },
   "create":{
      "__id":"create",
      "name":"Create",
      "functionName":"execute",
      "crudType":"C",
      "dataFlow":"push",
      "httpOptions":{
         "method":"post",
         "preProcessor":{
            "uriPath":"/orders",
            "isSingleCreate":true,
            "payload":{
               "structure":{
                    "customerId":"${customerId}",
                    "email":"${email}",
                    "customerName":"${customerName}",
                    "itemId":"${itemId}",
                    "itemName":"${itemName}",
                    "itemQuantity":"${itemQuantity}",
                    "itemPrice":"${itemPrice}",
                    "totalPrice":"${totalPrice}",
                    "orderDate":"${orderDate}",
                    "status":"${status}"
               }
            }
         },
         "postProcessor":{
            "successStatus":[
               "200"
            ],
            "responseHandler":{
               "failure":{
                  "dataPath":"orders"
               }
            }
         }
      },
      "input":{
         "list":[
            "data",
            "options"
         ],
         "data":{
            "name":"data",
            "dataType":"array"
         },
         "options":{
            "name":"options",
            "dataType":"object"
         }
      }
   }
}