      "list": [
        "domainURL",
        "database",
        "username",
        "password"
      ],
      "domainURL": {
        "name": "Domain URL",
        "dataType": "string",
        "icon": "http",
        "required": true
      },
      "database": {
        "name": "Database",
        "dataType": "string",
        "icon": "storage",
        "required": true
      },
      "username": {
        "name": "Username",
        "dataType": "string",
        "icon": "person",
        "required": true
      },
      "password": {
        "name": "Password or API Key",
        "dataType": "password",
        "icon": "security",
        "required": true
      }
    },
    "functions": {
      "list": [
        "getobjects",
        "getattributes"
      ],
      "getobjects": {
        "name": "Get Objects",
