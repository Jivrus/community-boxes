        "name": "Domain URL",
        "dataType": "string",
        "icon": "http",
        "required": true
      },
      "database": {
        "name": "Database",
        "dataType": "string",
        "icon": "storage",
        "required": true
      },
      "username": {
        "name": "Username",
        "dataType": "string",
        "icon": "person",
        "required": true
      },
